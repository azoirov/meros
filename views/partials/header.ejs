        <div class="header-bottom">
            <nav class="header-bottom__nav">
                <ul class="header-bottom__ul">
                    <% if (categories.length > 0) { %>
                        <% for (let i = 0; i < categories.length; i++) { %>
                            <% if (i === 12) break; %>
                            <li class="header-bottom__li">
                                <a class="header-bottom__link"
                                   href="/category/<%= categories[i].slug %>"><%= categories[i].ru_name[0].toUpperCase() + categories[i].ru_name.substring(1) %></a>
                            </li>
                        <% } %>
                    <% } else { %>
                        <li>Bo`sh</li>
                    <% } %>
                </ul>
            </nav>
        </div>
        <nav class="bottom-nav">
            <ul class="bottom-nav__ul">
                <li class="bottom-nav__li">
                    <a class="bottom-nav__link <%= (path === '/') ? 'active' : '' %>" href="/">
                        <img src="/images/bottom-nav/home<%= (path === '/') ? '-active' : '' %>.svg" alt="icon">
                        <span>Главная</span>
                    </a>
                </li>
                <li class="bottom-nav__li">
                    <a class="bottom-nav__link <%= (path === '/categories') ? 'active' : '' %>" href="/categories">
